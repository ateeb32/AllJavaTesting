package com.qa.ateeb.AutomatedAssessment;

import org.openqa.selenium.WebElement;
import org.openqa.selenium.support.FindBy;

public class JenkinsUserCreatedPage {
	
	@FindBy(id = "people")
	private WebElement userFound;
	
	
	public boolean findUser() {
		
		if ((userFound.getText().contains("donkey")) || (userFound.getText().contains("Chris95")) || (userFound.getText().contains("donkey")) || (userFound.getText().contains("donkey"))) {
			
			return true;
		}
		
		else {
			
			return false;
			
		}
		
	}

}
